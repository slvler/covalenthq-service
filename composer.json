{
    "name": "slvler/covalent",
    "license": "MIT",
    "description": "Laravel covalenthq.com api service",
    "type": "package",
    "authors": [
        {
            "name": "slvler",
            "email": "slvler@proton.me"
        }
    ],
    "autoload": {
        "psr-4": {
