    "scripts": {
        "test": "vendor/bin/phpunit tests"
    },
    "extra": {
        "laravel": {
            "providers": [
                "slvler\\covalent\\CovalentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
